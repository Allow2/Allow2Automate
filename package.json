{
  "name": "Allow2Automate",
  "productName": "Allow2Automate",
  "version": "2.0.0",
  "description": "Using Allow2 to control access and quotas for electricity via home automation devices",
  "main": "init.js",
  "author": {
    "name": "Allow2CEO",
    "email": "ceo@allow2.com"
  },
  "repository": "https://github.com/Allow2/Allow2Automate",
  "license": "CUSTOM",
  "dependencies": {
    "@atom/source-map-support": "^0.3.4",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "allow2": "^1.0.0",
    "async": "^2.6.3",
    "babel-runtime": "^6.22.0",
    "dialogs": "^1.1.20",
    "electron-plugin-manager": "^1.1.0",
    "electron-redux": "^2.0.0-alpha.9",
    "electron-settings": "^4.0.2",
    "firebase": "^12.7.0",
    "fs-path": "0.0.24",
    "fs-plus": "^3.1.1",
    "history": "^4.10.1",
    "marked": "^4.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "node-localstorage": "3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^5.1.2",
    "react-router": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.5",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reselect": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.1.18",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-mocha": "^12.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^2.7.1",
    "sinon": "^21.0.1"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "develop": "npm run private:compile -- --source-maps true && run-p -r private:watch private:serve",
    "test": "npm run lint:analytics && electron-mocha --renderer -R spec --require babel-core/register test/**/*.spec.js",
    "lint": "eslint --no-ignore scripts app test *.js",
    "lint:analytics": "node scripts/check-analytics-compliance.js --all",
    "lint:analytics:strict": "node scripts/check-analytics-compliance.js --all --strict",
    "prepack": "npm run lint:analytics:strict",
    "icons": "electron-icon-maker --input=./images/icon.png --output=./app/assets",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "private:build:all": "electron-builder -mwl",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:build:linux": "electron-builder --linux",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel app/ --copy-files --out-dir build",
    "private:clean": "rimraf build",
    "prepare": "husky install"
  }
}
